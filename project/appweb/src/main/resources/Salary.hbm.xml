<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="vasilizas.bean.db.Salary" table="salary">

        <id name="id" type="int">
            <column name="id"/>
            <generator class="increment"/>
        </id>

        <property name="salary" column="salary"/>
        <property name="getSalid" column="salaryid"/>

        <!--        <many-to-one name="id" class="vasilizas.bean.db.TeacherDb"/>-->

    </class>
</hibernate-mapping>